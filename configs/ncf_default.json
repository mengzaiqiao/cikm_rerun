{
    "model": "ncf",
    "config_id": "default",
    "root_dir": "../",
    "dataset": "ml_100k",
    "data_split": "leave_one_out",
    "percent": 1,
    "emb_dim": 64,
    "num_negative": 4,
    "batch_size": 256,
    "metrics": ["ndcg_at_k", "precision_at_k", "recall_at_k", "map_at_k"],
    "validate_metric": "ndcg_at_k@10",
    "device_str": "gpu",
    "optimizer": "adam",
    "lr": 1e-3,
    "max_epoch": 20,
    "result_file": "result.csv",
    "log_dir": "logs/",
    "result_dir": "results/",
    "checkpoint_dir": "checkpoints/",
    "dataset_dir": "datasets/",
    "run_dir": "runs/",
    "gmf_config": {"name": "gmf", "latent_dim": 32, "save_name": "gmf.model"},
    "mlp_config": {
        "name": "mlp",
        "latent_dim": 32,
        "layers": [64, 32, 16, 8],
        "save_name": "mlp.model",
        "pretrain_gmf": "gmf.model"
    },
    "neumf_config": {
        "name": "ncf",
        "latent_dim_gmf": 32,
        "latent_dim_mlp": 32,
        "layers": [64, 32, 16, 8],
        "pretrain_gmf": "gmf.model",
        "pretrain_mlp": "mlp.model",
        "save_name": "ncf.model"
    }
}
